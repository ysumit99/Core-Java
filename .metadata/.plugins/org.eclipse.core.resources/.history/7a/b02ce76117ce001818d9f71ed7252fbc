package basic_java;

public class Cube {
	int length;
	int breadth;
	int height;
	
	public int getCubeVolume() {
		return (length*breadth*height);
	}
	
	// Default Constructor => never returns any value
	Cube () {
		
		length = 10;
		breadth = 20;
		height = 30;
		
	}
	
	//Parameterized Constructor
	Cube (int l, int b, int h) {
			
			length = 10;
			breadth = 20;
			height = 30;
			
		}
}
